{
  "description": "Example Node.js application",
  "repository": "github:demo-source/node-app",
  "license": "Unlicense",
  "type": "module",
  "engines": {
    "node": ">=13"
  },
  "exports": {
    ".": "./lib-sample/example.js"
  },
  "scripts": {
    "test": "node test",
    "coverage": "nyc node test"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "latest",
    "@babel/plugin-transform-modules-commonjs": "latest",
    "@babel/register": "latest",
    "babel-eslint": "latest",
    "babel-hook-esm-to-cjs-package-type-runtime": "latest",
    "babel-plugin-istanbul": "latest",
    "eslint": "latest",
    "eslint-config-standard": "latest",
    "eslint-plugin-import": "latest",
    "eslint-plugin-node": "latest",
    "eslint-plugin-promise": "latest",
    "eslint-plugin-standard": "latest",
    "nyc": "latest"
  },
  "eslintIgnore": [],
  "babel": {
    "plugins": [
      "babel-plugin-istanbul",
      "@babel/plugin-transform-modules-commonjs",
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "nyc": {
    "require": [
      "babel-hook-esm-to-cjs-package-type-runtime",
      "@babel/register"
    ],
    "reporter": [
      "lcov",
      "text",
      "html"
    ],
    "sourceMap": false,
    "instrument": false
  }
}
